{
    "version": 2,
    "builds": [
        {
            "src": "./app.js", 
            "use": "@vercel/node"
        },
        {
            "src": "./cellpoint/build", 
            "use": "@vercel/static"
        }
    ],
    
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/app.js"
        }
    ]
}